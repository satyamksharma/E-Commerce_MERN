{
    "name": "digi-shop",
    "version": "1.0.0",
    "description": "An Online Shopping webApp built using MERN stack",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node backend/server.js",
        "server": "nodemon backend/server.js",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/seeder.js",
        "data:destroy": "node backend/seeder.js -d"
    },
    "author": "Satyam Kumar",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "express": "^4.18.2",
        "mongoose": "^8.0.4"
    },
    "devDependencies": {
        "dotenv": "^16.3.1",
        "nodemon": "^3.0.2"
    }
}
